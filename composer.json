    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "vlinde\\helper\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "vlinde\\helper\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "vlinde\\helper\\helperServiceProvider"
            ],
            "aliases": {
                "helper": "vlinde\\helper\\Facades\\helper"
            }
        }
    }
}
